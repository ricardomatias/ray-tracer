{
    "cmake.configureOnOpen": true,
    "files.associations": {
        "Makefile.*": "makefile",
        "*.shader": "glsl",
        "iosfwd": "cpp",
        "__config": "cpp",
        "istream": "cpp",
        "iostream": "cpp",
        "ios": "cpp",
        "__locale": "cpp"
    },
    "C_Cpp.errorSquiggles": "Enabled",
    "cmake.installPrefix": "${workspaceRoot}/bin"
}
